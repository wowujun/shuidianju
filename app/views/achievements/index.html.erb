<%=render 'head'%>
<%=render 'proleft'%>


<p id="notice"><%= notice %></p>

<h1>Listing Achievements</h1>

<table>
  <thead>
    <tr>
      <th>Projectitem</th>
      <th>Time</th>
      <th>Contractsum</th>
      <th>Owner</th>
      <th>Proabstract</th>
      <th>Contractcla</th>
      <th>Nation</th>
      <th>Enexploit</th>
      <th>Cla</th>
      <th>Bidletter</th>
      <th>Protocal</th>
      <th>Completcert</th>
      <th>Owncert</th>
      <th>Receport</th>
      <th>Consortium</th>
      <th>Image</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @achievements.each do |achievement| %>
      <tr>
        <td><%= achievement.projectitem %></td>
        <td><%= achievement.time %></td>
        <td><%= achievement.contractsum %></td>
        <td><%= achievement.owner %></td>
        <td><%= achievement.proabstract %></td>
        <td><%= achievement.contractcla %></td>
        <td><%= achievement.nation %></td>
        <td><%= achievement.enexploit %></td>
        <td><%= achievement.cla_id %></td>
        <td><%= link_to 'Show', achievement %></td>
        <td><%= link_to 'Edit', edit_achievement_path(achievement) %></td>
        <td><%= link_to 'Destroy', achievement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Achievement', new_achievement_path %>
